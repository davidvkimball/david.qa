filters:
  and:
    - '!file.name.startsWith("_")'
    - file.ext.startsWith("md")
    - '!file.name.contains("template")'
properties:
  note.title:
    displayName: Title
  note.pubDate:
    displayName: Date
views:
  - type: cards
    name: Cards
    order:
      - title
      - pubDate
  - type: table
    name: Table
    order:
      - pubDate
      - title
      - file.name
    columnSize:
      note.pubDate: 118
      note.title: 286
